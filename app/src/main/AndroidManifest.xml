<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectopdam">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListaCursos"
            android:label="@string/title_activity_lista_cursos" />
        <activity
            android:name=".CrearUsuario"
            android:label="@string/title_activity_crear_usuario" />
        <activity
            android:name=".PerfilUsuario"
            android:label="@string/title_activity_perfil" />
        <activity
            android:name=".PerfilCurso"
            android:label="@string/title_activity_perfil_curso" />
        <activity
            android:name=".VentaTickets"
            android:label="@string/title_activity_venta_tickets"></activity>
    </application>

</manifest>